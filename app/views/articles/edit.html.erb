<h1>Edit Article</h1>

<%= form_with model: @article do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :content %>
  <%= form.text_area :content %>
  <%= form.submit "Update Article" %>
<% end %>

<a href="<%= articles_path %>">Back</a>
<a href="<%= article_path(@article) %>">Show</a>
